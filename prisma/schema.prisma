generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgresql://pandey:pandey@localhost:5432/postgres")
}

model Image {
  id          String   @id @default(uuid())
  url         String
  description String
  embedding   Bytes
  createdAt   DateTime @default(now())
}
